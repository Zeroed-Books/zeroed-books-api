    },
    "query": "\n            SELECT e.\"currency\", COALESCE(SUM(e.\"amount\"), 0) AS \"amount!\"\n                FROM transaction_entry e\n                    JOIN account a ON a.id = e.account_id\n                    JOIN transaction t ON t.id = e.transaction_id\n            WHERE\n                t.legacy_user_id = $1\n                AND\n                    (a.name = $2 OR a.name LIKE $2 || ':%')\n            GROUP BY e.currency\n            ORDER BY e.currency\n            "
  },
